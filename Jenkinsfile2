pipeline {

    agent any  {
}
stages {
    stage('Git Checkout'){
        steps{
            script {
                git branch: 'develop', url: 'https://github.com/khadar099/Judy_app.git'
            }
        }
    }
    stage('build') {
        steps {
            script {
                sh ' mvn clean install'
            }
        }
    }
}
}
